    "def num(num1, num2):\n",
    "    if num1 > num2 :\n",
    "        high_num = num1\n",
    "    else :\n",
    "        high_num = num2\n",
    "\n",
    "    while True:\n",
    "        if high_num % num1 == 0 and high_num % num2 == 0 :\n",
    "            return high_num\n",
    "        high_num += 1\n",
    "\n",
    "print(num(1071, 1029))"
   ]
